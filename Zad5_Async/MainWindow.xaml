<Window x:Class="Zad5_Async.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zad5_Async"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="2*" />
        </Grid.ColumnDefinitions>
        <StackPanel Name="StartButtons" Grid.Column="0">
        </StackPanel>
        <StackPanel Name="StopButtons" Grid.Column="1">
        </StackPanel>
        <TextBox Name="OutputBox" Grid.Column="2" Margin="10" FontSize="12" FontFamily="Consolas" TextWrapping="Wrap" ></TextBox>
    </Grid>
</Window>
